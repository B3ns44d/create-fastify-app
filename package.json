{
  "name": "create-fastify-app",
  "version": "1.5.12",
  "description": "A Fastify application generator",
  "main": "create-fastify-app.js",
  "bin": {
    "fastify-app": "create-fastify-app.js",
    "fastify-repl": "./lib/repl/repl-client.js"
  },
  "scripts": {
    "lint": "standard --verbose",
    "unit": "tap test/*.test.js --no-coverage-report --jobs=1",
    "coverage": "tap test/*.test.js --coverage && tap --coverage-report=text-lcov | coveralls",
    "test": "npm run lint && npm run unit"
  },
  "keywords": [
    "fastify",
    "fastify-swagger",
    "fastify-plugin",
    "nodejs",
    "nodejs-library",
    "redis",
    "cors",
    "mongodb"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidedantonio/create-fastify-app.git"
  },
  "author": "Davide D'Antonio <davide.dantonio@webeetle.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidedantonio/create-fastify-app/issues"
  },
  "homepage": "https://github.com/davidedantonio/create-fastify-app#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "chokidar": "^3.0.0",
    "clui": "^0.3.6",
    "codecov": "^3.2.0",
    "command-exists": "^1.2.8",
    "commist": "^1.0.0",
    "fastify": "^2.4.1",
    "fastify-plugin": "^1.6.0",
    "generify": "^3.0.2",
    "handlebars": "^4.1.2",
    "inquirer": "^6.2.2",
    "js-beautify": "^1.8.9",
    "js-yaml": "^3.13.1",
    "json-schema-ref-parser": "^6.1.0",
    "lodash": "^4.17.11",
    "make-promises-safe": "^5.0.0",
    "pm2-axon": "^3.3.0",
    "pm2-axon-rpc": "^0.5.1",
    "portfinder": "^1.0.20",
    "pretty-bytes": "^5.2.0",
    "swagger-parser": "^6.0.5",
    "table": "^5.4.0",
    "tiny-human-time": "^1.2.0",
    "yargs-parser": "^13.0.0"
  },
  "devDependencies": {
    "concat-stream": "^2.0.0",
    "coveralls": "^3.0.3",
    "dotenv": "^8.0.0",
    "fastify-autoload": "^0.8.0",
    "fastify-cors": "^2.1.3",
    "fastify-mongodb": "^1.0.1",
    "fastify-mysql": "^0.3.0",
    "fastify-postgres": "^2.0.0",
    "fastify-redis": "^3.1.0",
    "fastify-swagger": "^2.4.0",
    "is-docker": "^2.0.0",
    "mkdirp": "^0.5.1",
    "pg": "^7.9.0",
    "pino-colada": "^1.4.4",
    "pump": "^3.0.0",
    "resolve-from": "^5.0.0",
    "rimraf": "^2.6.3",
    "simple-get": "^3.0.3",
    "standard": "^12.0.1",
    "tap": "^14.1.11",
    "walker": "^1.0.7"
  },
  "greenkeeper": {
    "ignore": [
      "request"
    ]
  }
}
