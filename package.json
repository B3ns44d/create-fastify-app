{
  "name": "create-fastify-app",
  "version": "1.6.12",
  "description": "A Fastify application generator",
  "main": "create-fastify-app.js",
  "bin": {
    "create-fastify-app": "create-fastify-app.js"
  },
  "scripts": {
    "lint": "standard --verbose",
    "unit": "tap test/*.test.js --no-coverage-report --jobs=1",
    "coverage": "tap test/*.test.js --coverage && tap --coverage-report=text-lcov | coveralls",
    "test": "npm run lint && npm run unit"
  },
  "keywords": [
    "fastify",
    "fastify-swagger",
    "fastify-plugin",
    "nodejs",
    "nodejs-library",
    "redis",
    "cors",
    "mongodb"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidedantonio/fastify-generator.git"
  },
  "author": "Davide D'Antonio <davide.dantonio@webeetle.com>",
  "contributors": [
    {
      "name": "Simone Gentili",
      "email": "sensorario@gmail.com",
      "url": "https://github.com/sensorario"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidedantonio/fastify-generator/issues"
  },
  "homepage": "https://github.com/davidedantonio/fastify-generator#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "codecov": "^3.5.0",
    "command-exists": "^1.2.8",
    "commist": "^1.1.0",
    "fastify": "^2.8.0",
    "fastify-plugin": "^1.6.0",
    "generify": "^3.0.3",
    "handlebars": "^4.1.2",
    "inquirer": "^7.0.0",
    "js-beautify": "^1.8.9",
    "js-yaml": "^3.13.1",
    "json-schema-ref-parser": "^7.1.1",
    "lodash": "^4.17.15",
    "make-promises-safe": "^5.0.0",
    "swagger-parser": "^8.0.1",
    "yargs-parser": "^13.1.1"
  },
  "devDependencies": {
    "concat-stream": "^2.0.0",
    "coveralls": "^3.0.6",
    "dotenv": "^8.1.0",
    "fastify-autoload": "^1.0.0",
    "fastify-cors": "^2.1.3",
    "fastify-mongodb": "^2.0.0",
    "fastify-mysql": "^0.3.0",
    "fastify-postgres": "^2.0.0",
    "fastify-redis": "^3.1.1",
    "fastify-swagger": "^2.4.0",
    "is-docker": "^2.0.0",
    "mkdirp": "^0.5.1",
    "pg": "^7.12.1",
    "pino-colada": "^1.4.5",
    "pump": "^3.0.0",
    "resolve-from": "^5.0.0",
    "rimraf": "^3.0.0",
    "simple-get": "^3.0.3",
    "standard": "^14.1.0",
    "tap": "^14.6.1",
    "walker": "^1.0.7"
  },
  "greenkeeper": {
    "ignore": [
      "request"
    ]
  }
}
